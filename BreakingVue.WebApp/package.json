    "scripts": {
        "dev": "gulp debug",
        "build": "npm run compile:prod && npm run test",
        "compile:prod": "webpack --mode=production --config wwwroot/config/webpack.config.prod.js",
        "compile:dev": "webpack --mode=development --watch --config wwwroot/config/webpack.config.dev.js",
        "compile:test": "webpack --mode=development --watch --config wwwroot/config/webpack.config.test.js",
        "test": "karma start wwwroot/config/karma.unit.js",
        "test:debug": "karma start wwwroot/config/karma.debug.js",
        "test:watch": "karma start wwwroot/config/karma.unit.js --singleRun=false --auto-watch"
    },
